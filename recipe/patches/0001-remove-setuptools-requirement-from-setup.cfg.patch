Subject: [PATCH] remove setuptools requirement from setup.cfg

---
 setup.cfg | 2 --
 1 file changed, 2 deletions(-)

diff --git a/setup.cfg b/setup.cfg
index d51dd16..12e1247 100644
--- a/setup.cfg
+++ b/setup.cfg
@@ -25,8 +25,6 @@ zip_safe = False
 python_requires=>=2.7, !=3.0.*, !=3.1.*, !=3.2.*, !=3.3.*
 include_package_data = True
 packages = varlink
-setup_requires =
-  setuptools_scm
 
 [options.package_data]
 varlink = *.varlink
